<div class="row">
    <div class="col s12">
        <% for(var i = 0; i < charada.length; i++) { %>
            <% var n = i < 9 ? "0" + (i + 1) : (i + 1); %>
            <div class="charada-item valign-wrapper left-align">
                <div class="col s3">
                    <div class="img" style="<%= getImage(n-1, "{{APP_SERVICE_PATH}}/images", 60) %>"></div>
                </div>
                <div class="col s4">
                    <h1>#<%= n %></h1>
                </div>
                <div class="col s4">
					<h2><%= charada[i] %></h2>
				</div>
                <div class="col s1"></div>
            </div>
        <% } %>
    </div>
</div>

<div class="fixed-action-btn hide-on-unique">
    <a class="btn-floating btn-large waves-effect waves-light green" href="#!"
       onclick="apretaste.send({'command':'BOLITA'})"><i class="material-icons">arrow_back</i></a>
</div>