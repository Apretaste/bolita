<div class="row">
	<div class="col s12">
		<h1>Ãšltimos 7 resultados</h1>

		<h2>El Fijo</h2>
		<table class="striped">
			<tr>
				<th>Pick3</th>
				<th class="center-align">Fijo</th>
			</tr>
			<% lastResultsP3.forEach(function(resultadoP3){ %>
			<tr>
				<td><%= resultadoP3.NumGanador %></td>
				<td class="center-align">
					<b><%= resultadoP3.fijo %></b><br>
					<small><%= resultadoP3.charada %></small>
				</td>
			</tr>
			<% }) %>
		</table>

		<h2>Corridos</h2>
		<table class="striped">
			<tr>
				<th>Pick4</th>
				<th class="center-align">Corrido 1</th>
				<th class="center-align">Corrido 2</th>
			</tr>

			<% lastResultsP4.forEach(function(resultadoP4){ %>
			<tr>
				<td><%= resultadoP4.NumGanador %></td>
				<td class="center-align">
					<b><%=resultadoP4.corrido1 %></b><br>
					<small><%= resultadoP4.charada1%></small>
				</td>
				<td class="center-align">
					<b><%=resultadoP4.corrido2 %></b><br>
					<small><%= resultadoP4.charada2%></small>
				</td>
			</tr>
			<% }) %>
		</table>
	</div>
</div>

